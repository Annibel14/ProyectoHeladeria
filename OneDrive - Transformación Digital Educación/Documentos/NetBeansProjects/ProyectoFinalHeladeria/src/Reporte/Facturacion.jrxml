	<parameter name="NoFactura" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     detalle_factura.`id_factura` AS detalle_factura_id_factura,
     detalle_factura.`id_producto` AS detalle_factura_id_producto,
     detalle_factura.`cantidad_vendida` AS detalle_factura_cantidad_vendida,
     detalle_factura.`importe` AS detalle_factura_importe,
     facturacion.`NoFactura` AS factura_no_factura,
     facturacion.`NombreCliente` AS factura_nombre_cliente,
     facturacion.`Cajera` AS factura_cajera,
     facturacion.`FormaPago` AS factura_tipo_pago,
     facturacion.`Total` AS factura_Total,
     facturacion.`Fecha_factura` AS fecha_factura,
     producto.`Codigo_producto` AS producto_id_producto,
     producto.`Producto` AS producto_nombre_producto,
     
     producto.`Precio` AS producto_unidad_precio
    
FROM
    facturacion
     INNER JOIN `detalle_factura` AS detalle_factura ON facturacion.`NoFactura` =  detalle_factura.`id_factura`
     INNER JOIN `producto` AS producto ON detalle_factura.`id_producto` = producto.`Codigo_producto`
     where NoFactura = $P{NoFactura}]]>
	</queryString>
